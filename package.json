{
  "name": "esbern-org-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "node --env-file=.env.test --import tsx --test tests/**/*.test.ts",
    "lint": "eslint",
    "dev": "rimraf ./build && npm run build-sql && tsc-watch --project tsconfig.json --outDir ./build --onSuccess \"node ./build/main.js\"",
    "build": "rimraf ./build && tsc --project tsconfig.json --outDir ./build",
    "generate-crypt-key": "node ./build/cli/generate-crypto-key.js",
    "bootstrap": "node ./build/cli/bootstrap.js",
    "reset-admin-role": "node ./build/cli/reset-admin-role.js",
    "create-admin": "node ./build/cli/create-admin.js"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dayjs": "1.11.13",
    "express": "4.19.2",
    "joi": "17.13.3",
    "mysql2": "3.11.0",
    "uuidv7": "1.0.2",
    "winston": "3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "9.10.0",
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.17",
    "@types/eslint__js": "8.42.3",
    "@types/express": "4.17.21",
    "@types/node": "22.5.4",
    "@types/supertest": "6.0.2",
    "eslint": "9.9.1",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "supertest": "7.0.0",
    "ts-node": "10.9.2",
    "tsc-watch": "6.2.0",
    "tsx": "4.19.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.4.0"
  }
}
