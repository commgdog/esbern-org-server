{
  "name": "esbern-org-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env ./src/server.ts",
    "lint": "eslint --fix",
    "test": "vitest --run",
    "build": "tsx build.ts && tsc --project tsconfig.json --outDir ./build",
    "reset-administrator": "tsx --env-file=.env src/cli/reset-administrator.ts"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dayjs": "1.11.13",
    "express": "4.21.0",
    "joi": "17.13.3",
    "mysql2": "3.11.3",
    "prompt-sync": "4.2.0",
    "uuidv7": "1.0.2",
    "winston": "3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "9.11.1",
    "@stylistic/eslint-plugin": "2.8.0",
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "22.7.3",
    "@types/prompt-sync": "4.2.3",
    "@types/supertest": "6.0.2",
    "dotenv": "16.4.5",
    "eslint": "9.11.1",
    "supertest": "7.0.0",
    "tsx": "4.19.1",
    "typescript": "5.6.2",
    "typescript-eslint": "8.7.0",
    "vitest": "2.1.1"
  }
}
