{
  "name": "esbern-org-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest --run",
    "lint": "eslint",
    "dev": "rimraf ./build && tsc-watch --project tsconfig.json --outDir ./build --onSuccess \"node ./build/server.js\"",
    "build": "rimraf ./build && npm run build-sql && tsc --project tsconfig.json --outDir ./build",
    "build-sql": "npx copyfiles -f ./src/sql/* ./build/sql"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dayjs": "1.11.13",
    "express": "4.20.0",
    "joi": "17.13.3",
    "mysql2": "3.11.1",
    "uuidv7": "1.0.2",
    "winston": "3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "9.10.0",
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.17",
    "@types/eslint__js": "8.42.3",
    "@types/express": "4.17.21",
    "@types/node": "22.5.4",
    "@types/supertest": "6.0.2",
    "copyfiles": "2.4.1",
    "dotenv": "16.4.5",
    "eslint": "9.10.0",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "supertest": "7.0.0",
    "tsc-watch": "6.2.0",
    "typescript": "5.6.2",
    "typescript-eslint": "8.5.0",
    "vitest": "2.0.5"
  }
}
